namespace rand {
  func noise(x:f32,y:f32,z:f32):f32;
  func noise(x:f32,y:f32):f32{return noise(x,y,0)};
  func noise(x:f32):f32{return noise(x,0,0);}
  func noise_reseed();
  func noise_detail(lod:i32,falloff:f32);
  func random():f32;
  func random(a:f32):f32{return random()*a};
  func random[N](a:vec[f32,N]):vec[f32,N]{return a.map(func(x:f32):f32{return random()*x;})}
  func random[T](a:T,b:T):T{return a+random(b-a)};
  func seed(x:u32);
}
